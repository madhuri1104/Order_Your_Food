<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.orderyourfood">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Orderyourfood"
        tools:targetApi="31">
        <activity
            android:name=".Activity.RotiActivity"
            android:exported="true"
            android:parentActivityName=".Activity.MainActivity"/>
        <activity
            android:name=".Activity.Show_Cart_inside_Activity"
            android:exported="false" />
        <activity
            android:name=".Activity.ChickenActivity"
            android:exported="true"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.CurryActivity"
            android:exported="true"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.IceCreamActivity"
            android:exported="true"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.DrinkActivity"
            android:exported="true"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.RiceActivity"
            android:exported="true"
            android:parentActivityName=".Activity.MainActivity"/>
        <activity
            android:name=".Activity.BurgerActivity"
            android:exported="true"
            android:parentActivityName=".Activity.MainActivity"/>
        <activity
            android:name=".Activity.PizzaActivity"
            android:exported="true"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.InsideCategoryActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.PaymentActivity"
            android:exported="true"
            android:parentActivityName=".Activity.CartActivity" />
        <activity
            android:name=".Activity.ForgotPassActivity"
            android:exported="true"
            android:parentActivityName=".Activity.LoginActivity" />
        <activity
            android:name=".Activity.UpdateProfileActivity"
            android:exported="true"
            android:parentActivityName=".Activity.ProfileActivity" />
        <activity
            android:name=".Activity.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.SettingsActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.CartActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.ShowDetailsActivity"
            android:exported="true"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.FlashscreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.MainActivity"
            android:exported="true" />
    </application>

</manifest>